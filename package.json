{
  "name": "agent-profile",
  "version": "1.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --public-host na1.dev.localhost --port 4200  --configuration development",
    "build": "npm run lint && ng build --base-href /agent-profile/ --progress --configuration development",
    "build-prod": "npm run test && ng build --base-href /agent-profile/ --progress --configuration production",
    "build-dev": "npm run lint && npm run test && npm run build",
    "test": "ng test --progress --watch=false",
    "test:ghworkflow": "npm run build && npm run test",
    "lint": "eslint ./src --ext .js,.ts",
    "lint-fix": "eslint ./src --ext .js,.ts --fix",
    "e2e-suite_01": "npx playwright test --config=e2e/suite01/suite01.config.ts",
    "e2e-suite_02": "npx playwright test --config=e2e/suite02/suite02.config.ts",
    "e2e-suite_03": "npx playwright test --config=e2e/suite03/suite03.config.ts",
    "e2e-suite_04": "npx playwright test --config=e2e/suite04/suite04.config.ts",
    "e2e-all": "npm run e2e-suite_01 && npm run e2e-suite_02 && npm run e2e-suite_03 && npm run e2e-suite_04",
    "e2e-coverage-report": "npx nyc report --report-dir=target/reports/integration-coverage --reporter=lcov --reporter=text-summary --reporter=html --reporter=cobertura"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^17.1.0",
    "@angular/cdk": "^17.1.0",
    "@angular/common": "^17.1.0",
