    "@angular/platform-server": "^17.1.0",
    "@angular/router": "^17.1.0",
    "@angular/upgrade": "^17.1.0",
    "@ngx-translate/core": "^15.0.0",
    "@ngx-translate/http-loader": "^8.0.0",
    "ag-grid-angular": "^31.0.1",
    "ag-grid-community": "^31.0.1",
    "ag-grid-enterprise": "^31.0.1",
    "angular-i18next": "^17.0.0",
    "angular-resize-event": "~3.2.0",
    "bootstrap": "~3.4.0",
    "core-js": "3.23.1",
    "@niceltd/cxone-client-platform-services": "^20.1.0",
    "@niceltd/cxone-components": "^20.10.0",
